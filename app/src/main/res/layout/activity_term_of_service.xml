<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.login.TermOfServiceActivity">

  <View
      android:id="@+id/view12"
      android:layout_width="0dp"
      android:layout_height="0.5dp"
      android:layout_marginTop="14dp"
      android:background="#dbdbdb"
      app:layout_constraintEnd_toEndOf="@+id/view11"
      app:layout_constraintStart_toStartOf="@+id/view11"
      app:layout_constraintTop_toBottomOf="@+id/select_checkbox" />

  <View
      android:id="@+id/view13"
      android:layout_width="wrap_content"
      android:layout_height="12dp"
      android:layout_marginTop="20dp"
      android:background="#F2F5F5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/second_checkbox" />

  <TextView
      android:id="@+id/textView51"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:text="이메일"
      android:textColor="#727272"
      android:textSize="12sp"
      app:layout_constraintBottom_toBottomOf="@+id/fifth_checkbox"
      app:layout_constraintStart_toEndOf="@+id/fifth_checkbox"
      app:layout_constraintTop_toTopOf="@+id/fifth_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/fifth_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginStart="100dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintBottom_toBottomOf="@+id/fouth_checkbox"
      app:layout_constraintStart_toEndOf="@+id/textView47"
      app:layout_constraintTop_toTopOf="@+id/fouth_checkbox" />

  <TextView
      android:id="@+id/textView50"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:paddingTop="5dp"
      android:paddingBottom="5dp"
      android:text="이용약관"
      android:textColor="#727272"
      android:textSize="12sp"
      app:layout_constraintBottom_toBottomOf="@+id/second_checkbox"
      app:layout_constraintEnd_toStartOf="@+id/agreement_arrow_button"
      app:layout_constraintStart_toEndOf="@+id/second_checkbox"
      app:layout_constraintTop_toTopOf="@+id/second_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/second_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginTop="9dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintStart_toStartOf="@+id/first_checkbox"
      app:layout_constraintTop_toBottomOf="@+id/first_checkbox" />

  <TextView
      android:id="@+id/textView49"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:paddingTop="5dp"
      android:paddingBottom="5dp"
      android:text="상품 및 마케팅 이용동의"
      android:textColor="#727272"
      android:textSize="12sp"
      app:layout_constraintBottom_toBottomOf="@+id/third_checkbox"
      app:layout_constraintEnd_toStartOf="@+id/marketing_arrow_button"
      app:layout_constraintStart_toEndOf="@+id/third_checkbox"
      app:layout_constraintTop_toTopOf="@+id/third_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/select_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginTop="8dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintStart_toStartOf="@+id/textView53"
      app:layout_constraintTop_toBottomOf="@+id/textView53" />

  <TextView
      android:id="@+id/textView48"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:text="전체동의"
      android:textColor="#323232"
      android:textSize="12.7sp"
      app:layout_constraintBottom_toBottomOf="@+id/select_checkbox"
      app:layout_constraintStart_toEndOf="@+id/select_checkbox"
      app:layout_constraintTop_toTopOf="@+id/select_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/third_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginTop="17dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintStart_toStartOf="@+id/select_checkbox"
      app:layout_constraintTop_toBottomOf="@+id/view12" />

  <TextView
      android:id="@+id/textView47"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:text="SMS"
      android:textColor="#727272"
      android:textSize="12sp"
      app:layout_constraintBottom_toBottomOf="@+id/fouth_checkbox"
      app:layout_constraintStart_toEndOf="@+id/fouth_checkbox"
      app:layout_constraintTop_toTopOf="@+id/fouth_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/fouth_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginTop="9dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintStart_toStartOf="@+id/textView49"
      app:layout_constraintTop_toBottomOf="@+id/third_checkbox" />

  <TextView
      android:id="@+id/textView46"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:paddingTop="5dp"
      android:paddingBottom="5dp"
      android:text="개인정보처리방침"
      android:textColor="#727272"
      android:textSize="12sp"
      app:layout_constraintBottom_toBottomOf="@+id/first_checkbox"
      app:layout_constraintEnd_toStartOf="@+id/user_info_arrow_button"
      app:layout_constraintStart_toEndOf="@+id/first_checkbox"
      app:layout_constraintTop_toTopOf="@+id/first_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/first_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginTop="17dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintStart_toStartOf="@+id/necessary_checkbox"
      app:layout_constraintTop_toBottomOf="@+id/view11" />

  <View
      android:id="@+id/view10"
      android:layout_width="wrap_content"
      android:layout_height="137.3dp"
      android:background="#F2F5F5"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView134" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="20dp"
      android:fontFamily="@font/dream5"
      android:text="서비스 이용약관"
      android:textColor="#323232"
      android:textSize="15sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toTopOf="@+id/textView6"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/view10"
      app:layout_constraintVertical_bias="0.0"
      tools:ignore="HardcodedText" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="0dp"
      android:layout_height="wrap_content"
      android:layout_marginBottom="20dp"
      android:gravity="center"
      android:text="“뷰켓” 앱 이용을 위해\n다음 서비스 이용약관 동의가 필요합니다."
      android:textColor="#727272"
      android:textSize="13sp"
      app:layout_constraintBottom_toBottomOf="@+id/view10"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/textView5"
      tools:ignore="HardcodedText" />

  <TextView
      android:id="@+id/textView31"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="9dp"
      android:text="전체동의"
      android:textColor="#323232"
      android:textSize="12.7sp"
      app:layout_constraintBottom_toBottomOf="@+id/necessary_checkbox"
      app:layout_constraintStart_toEndOf="@+id/necessary_checkbox"
      app:layout_constraintTop_toTopOf="@+id/necessary_checkbox"
      tools:ignore="HardcodedText" />

  <CheckBox
      android:id="@+id/necessary_checkbox"
      android:layout_width="20dp"
      android:layout_height="20dp"
      android:layout_marginTop="8dp"
      android:background="@drawable/custom_checkbox"
      android:button="@null"
      app:layout_constraintStart_toStartOf="@+id/textView52"
      app:layout_constraintTop_toBottomOf="@+id/textView52" />

  <TextView
      android:id="@+id/textView52"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="16dp"
      android:layout_marginTop="13dp"
      android:fontFamily="@font/notosans_medium"
      android:text="필수약관"
      android:textColor="#323232"
      android:textSize="13.3sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/view10"
      tools:ignore="HardcodedText" />

  <TextView
      android:id="@+id/textView53"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="13dp"
      android:fontFamily="@font/notosans_medium"
      android:text="선택약관"
      android:textColor="#323232"
      android:textSize="13.3sp"
      app:layout_constraintStart_toStartOf="@+id/textView52"
      app:layout_constraintTop_toBottomOf="@+id/view13"
      tools:ignore="HardcodedText" />

  <View
      android:id="@+id/view11"
      android:layout_width="0dp"
      android:layout_height="0.5dp"
      android:layout_marginTop="14dp"
      android:layout_marginEnd="16dp"
      android:background="#dbdbdb"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="@+id/textView52"
      app:layout_constraintTop_toBottomOf="@+id/necessary_checkbox" />

  <TextView
      android:id="@+id/confirm_text"
      android:layout_width="0dp"
      android:layout_height="50dp"
      android:background="@color/main_theme"
      android:fontFamily="@font/dream5"
      android:gravity="center"
      android:text="확인"
      android:textColor="#ffffff"
      android:textSize="16sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      tools:ignore="HardcodedText" />

  <ImageView
      android:id="@+id/marketing_arrow_button"
      android:layout_width="6dp"
      android:layout_height="0dp"
      app:layout_constraintBottom_toBottomOf="@+id/textView49"
      app:layout_constraintEnd_toEndOf="@+id/view11"
      app:layout_constraintTop_toTopOf="@+id/textView49"
      app:srcCompat="@drawable/right_arrow"
      tools:ignore="ContentDescription" />

  <ImageView
      android:id="@+id/agreement_arrow_button"
      android:layout_width="6dp"
      android:layout_height="0dp"
      app:layout_constraintBottom_toBottomOf="@+id/textView50"
      app:layout_constraintEnd_toEndOf="@+id/view11"
      app:layout_constraintTop_toTopOf="@+id/textView50"
      app:srcCompat="@drawable/right_arrow"
      tools:ignore="ContentDescription" />

  <ImageView
      android:id="@+id/user_info_arrow_button"
      android:layout_width="6dp"
      android:layout_height="0dp"
      app:layout_constraintBottom_toBottomOf="@+id/textView46"
      app:layout_constraintEnd_toEndOf="@+id/view11"
      app:layout_constraintTop_toTopOf="@+id/textView46"
      app:srcCompat="@drawable/right_arrow"
      tools:ignore="ContentDescription" />

  <ImageView
      android:id="@+id/imageView134"
      android:layout_width="100dp"
      android:layout_height="53.3dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/left_arrow"
      tools:ignore="ContentDescription" />

  <TextView
      android:textColor="#323232"
      android:id="@+id/textView416"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="약관동의"
      app:layout_constraintBottom_toBottomOf="@+id/imageView134"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/imageView134"
      tools:ignore="HardcodedText" />
</androidx.constraintlayout.widget.ConstraintLayout>