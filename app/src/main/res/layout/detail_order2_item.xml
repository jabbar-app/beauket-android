<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/view70"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/order_detail_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view139"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/order_detail_background2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view70"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/view70"
        app:layout_constraintTop_toBottomOf="@+id/view70"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView106"
        android:layout_width="85dp"
        android:layout_height="85dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView172"
        app:layout_constraintTop_toBottomOf="@+id/view70"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView172"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="입점사명"
        android:textColor="#323232"
        app:layout_constraintBottom_toBottomOf="@+id/view70"
        app:layout_constraintStart_toStartOf="@+id/view70"
        app:layout_constraintTop_toTopOf="@+id/view70"
        tools:ignore="HardcodedText" />


    <TextView
        android:id="@+id/textView229"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="상태"
        android:textColor="#929292"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView172"
        app:layout_constraintEnd_toEndOf="@+id/view70"
        app:layout_constraintTop_toTopOf="@+id/textView172"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView230"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/stroke_gray"
        android:gravity="center"
        android:paddingTop="7dp"
        android:paddingBottom="7dp"
        android:text="주문취소"
        android:textColor="#929292"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView397"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/imageView106"
        app:layout_constraintTop_toBottomOf="@+id/imageView106"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView232"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text="[로우퀘스트]"
        android:textColor="#929292"
        android:textSize="13sp"
        app:layout_constraintBottom_toTopOf="@+id/textView233"
        app:layout_constraintStart_toEndOf="@+id/imageView106"
        app:layout_constraintTop_toTopOf="@+id/imageView106"
        app:layout_constraintVertical_chainStyle="packed"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView233"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="15dp"
        android:text="에키네시아 카밍 모이스쳐 크림"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView281"
        app:layout_constraintEnd_toEndOf="@+id/view139"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView232"
        app:layout_constraintTop_toBottomOf="@+id/textView232"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView281"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="45,000원"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView106"
        app:layout_constraintStart_toStartOf="@+id/textView232"
        app:layout_constraintTop_toBottomOf="@+id/textView233"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView394"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="1개"
        android:textColor="#929292"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView396"
        app:layout_constraintStart_toEndOf="@+id/textView396"
        app:layout_constraintTop_toTopOf="@+id/textView396"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView395"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/stroke_green"
        android:paddingLeft="5dp"
        android:paddingTop="3dp"
        android:paddingRight="5dp"
        android:paddingBottom="3dp"
        android:text="장바구니 담기"
        android:textColor="@color/main_theme"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView281"
        app:layout_constraintEnd_toEndOf="@+id/textView229"
        app:layout_constraintTop_toTopOf="@+id/textView281"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView396"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="ㅣ"
        android:textColor="#DBDBDB"
        app:layout_constraintBottom_toBottomOf="@+id/textView281"
        app:layout_constraintStart_toEndOf="@+id/textView281"
        app:layout_constraintTop_toTopOf="@+id/textView281"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView397"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:background="@drawable/stroke_gray"
        android:gravity="center"
        android:paddingTop="7dp"
        android:paddingBottom="7dp"
        android:text="배송조회"
        android:textColor="#929292"
        app:layout_constraintBottom_toBottomOf="@+id/textView230"
        app:layout_constraintEnd_toEndOf="@+id/textView229"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView230"
        tools:ignore="HardcodedText" />

    <TextView

        android:id="@+id/textView398"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/stroke_green"
        android:fontFamily="@font/notosans_regular"
        android:gravity="center"
        android:includeFontPadding="false"
        android:paddingTop="8dp"
        android:paddingBottom="8dp"
        android:text="구매후기 쓰기"
        android:textColor="@color/main_theme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView397"
        app:layout_constraintStart_toStartOf="@+id/textView230"
        app:layout_constraintTop_toBottomOf="@+id/textView230"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText" />

    <TextView
        android:visibility="gone"
        android:id="@+id/textView415"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/stroke_gray"
        android:gravity="center"
        android:paddingTop="7dp"
        android:paddingBottom="7dp"
        android:text="주문취소"
        android:textColor="#929292"
        app:layout_constraintEnd_toEndOf="@+id/textView229"
        app:layout_constraintStart_toStartOf="@+id/imageView106"
        app:layout_constraintTop_toTopOf="@+id/textView230"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>