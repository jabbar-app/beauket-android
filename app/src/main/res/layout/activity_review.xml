<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.ReviewActivity">


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/back_button"
        android:layout_width="70dp"
        android:layout_height="53.3dp"
        android:background="@drawable/left_arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView206"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="리뷰"
        android:textColor="#323232"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/back_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/back_button"
        tools:ignore="HardcodedText" />

    <View
        android:id="@+id/view63"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:background="#F2F5F5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/back_button" />

    <TextView
        android:id="@+id/textView207"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:text="리뷰"
        android:textColor="#323232"
        android:textSize="15sp"
        android:fontFamily="@font/dream5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view63"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView208"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        android:text="100"
        android:textColor="#059899"
        android:textSize="15sp"
        android:fontFamily="@font/dream5"
        app:layout_constraintBottom_toBottomOf="@+id/textView207"
        app:layout_constraintStart_toEndOf="@+id/textView207"
        app:layout_constraintTop_toTopOf="@+id/textView207"
        tools:ignore="HardcodedText" />

    <View
        android:id="@+id/view65"
        android:layout_width="10dp"
        android:layout_height="13dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="1dp"
        android:background="@drawable/right_arrow"
        android:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/textView207"
        app:layout_constraintStart_toEndOf="@+id/textView208"
        app:layout_constraintTop_toTopOf="@+id/textView207" />

    <View
        android:id="@+id/view66"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/border_radius_shadow"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView207" />

    <TextView
        android:id="@+id/textView210"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4.35"
        android:textColor="#000000"
        android:textSize="25sp"
        android:fontFamily="@font/noto_bold"
        android:includeFontPadding="false"
        app:layout_constraintBottom_toBottomOf="@+id/view66"
        app:layout_constraintEnd_toEndOf="@+id/view66"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="@+id/view66"
        app:layout_constraintTop_toTopOf="@+id/view66"
        app:layout_constraintVertical_bias="0.35"
        tools:ignore="HardcodedText" />

    <RatingBar
        android:isIndicator="true"
        android:id="@+id/ratingBar"
        style="@style/RatingBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:numStars="5"
        android:rating="5"
        android:scaleX="1.3"
        android:scaleY="1.5"
        app:layout_constraintEnd_toEndOf="@+id/textView210"
        app:layout_constraintStart_toStartOf="@+id/textView210"
        app:layout_constraintTop_toBottomOf="@+id/textView210" />

    <TextView
        android:id="@+id/textView211"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="5점"
        app:layout_constraintEnd_toEndOf="@+id/progressBar6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/progressBar6"
        app:layout_constraintTop_toBottomOf="@+id/progressBar6"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView214"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="3점"
        app:layout_constraintBottom_toBottomOf="@+id/textView212"
        app:layout_constraintEnd_toEndOf="@+id/progressBar8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/progressBar8"
        app:layout_constraintTop_toBottomOf="@+id/progressBar8"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView215"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="2점"
        app:layout_constraintEnd_toEndOf="@+id/progressBar10"
        app:layout_constraintStart_toStartOf="@+id/progressBar10"
        app:layout_constraintTop_toBottomOf="@+id/progressBar10"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView213"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="1점"
        app:layout_constraintEnd_toEndOf="@+id/progressBar9"
        app:layout_constraintStart_toStartOf="@+id/progressBar9"
        app:layout_constraintTop_toBottomOf="@+id/progressBar9"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView212"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="4점"
        app:layout_constraintBottom_toBottomOf="@+id/textView211"
        app:layout_constraintEnd_toEndOf="@+id/progressBar7"
        app:layout_constraintStart_toStartOf="@+id/progressBar7"
        app:layout_constraintTop_toBottomOf="@+id/progressBar7"
        tools:ignore="HardcodedText" />

    <ProgressBar
        android:id="@+id/progressBar6"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:max="5"
        android:padding="-100dp"
        android:progress="4"
        android:progressTint="#FE614E"
        android:rotation="270"
        app:layout_constraintStart_toEndOf="@+id/ratingBar"
        app:layout_constraintTop_toTopOf="@+id/textView210" />

    <TextView
        android:id="@+id/textView216"
        android:layout_width="25dp"
        android:layout_height="16dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/reply"
        android:gravity="center"
        android:text="63"
        android:textColor="@color/white"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/progressBar6"
        app:layout_constraintEnd_toEndOf="@+id/progressBar6"
        app:layout_constraintStart_toStartOf="@+id/progressBar6"
        tools:ignore="HardcodedText,SmallSp" />

    <ProgressBar
        android:id="@+id/progressBar7"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:max="5"
        android:padding="-100dp"
        android:progress="3"
        android:progressTint="#FE614E"
        android:rotation="270"
        app:layout_constraintStart_toStartOf="@+id/progressBar6"
        app:layout_constraintTop_toTopOf="@+id/progressBar6" />

    <ProgressBar
        android:id="@+id/progressBar8"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:max="5"
        android:padding="-100dp"
        android:progress="3"
        android:progressTint="#FE614E"
        android:rotation="270"
        app:layout_constraintStart_toStartOf="@+id/progressBar7"
        app:layout_constraintTop_toTopOf="@+id/progressBar6" />

    <ProgressBar
        android:id="@+id/progressBar9"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:max="5"
        android:progress="1"
        android:progressTint="#FE614E"
        android:rotation="270"
        app:layout_constraintStart_toStartOf="@+id/progressBar10"
        app:layout_constraintTop_toTopOf="@+id/progressBar6" />

    <ProgressBar
        android:id="@+id/progressBar10"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="65dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:max="5"
        android:padding="-100dp"
        android:progress="2"
        android:progressTint="#FE614E"
        android:rotation="270"
        app:layout_constraintStart_toStartOf="@+id/progressBar8"
        app:layout_constraintTop_toTopOf="@+id/progressBar6" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/review_recyclerview"
        android:layout_width="411dp"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view66" />

</androidx.constraintlayout.widget.ConstraintLayout>