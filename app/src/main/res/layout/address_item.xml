<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="120dp"
    android:layout_marginLeft="15dp"
    android:layout_marginTop="10dp"
    android:layout_marginRight="15dp"
    android:background="@drawable/stroke_green">

    <TextView
        android:id="@+id/textView294"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:background="@color/main_theme"
        android:gravity="center"
        android:padding="3dp"
        android:text="도로명"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintBottom_toTopOf="@+id/view101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView359"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:background="@color/main_theme"
        android:gravity="center"
        android:padding="3dp"
        android:text="지번"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView294"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/textView294"
        app:layout_constraintTop_toBottomOf="@+id/view101"
        tools:ignore="HardcodedText" />

    <View
        android:id="@+id/view101"
        android:layout_width="0dp"
        android:layout_height="0.5dp"
        android:background="#777777"
        app:layout_constraintBottom_toTopOf="@+id/textView359"
        app:layout_constraintEnd_toStartOf="@+id/view105"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/textView294"
        app:layout_constraintTop_toBottomOf="@+id/textView294" />

    <View
        android:id="@+id/view105"
        android:layout_width="0.5dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="65dp"
        android:background="#777777"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/zipNo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12345"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view101"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

    <TextView
        android:textSize="13sp"
        android:id="@+id/roadAddr"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:text="서울 송파구 잠실동 231-4202 럭키빌 202호"
        app:layout_constraintBottom_toBottomOf="@+id/textView294"
        app:layout_constraintEnd_toEndOf="@+id/view101"
        app:layout_constraintStart_toEndOf="@+id/textView294"
        app:layout_constraintTop_toTopOf="@+id/textView294"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/jibunAddr"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:text="TextView"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView359"
        app:layout_constraintEnd_toEndOf="@+id/view101"
        app:layout_constraintStart_toEndOf="@+id/textView359"
        app:layout_constraintTop_toTopOf="@+id/textView359"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>